\{ "OpenAI API key" } ->

let key = .'OpenAI API key'

let concatSep = import github
      { owner: "Gabriella439"
      , repository: "grace"
      , path: "prelude/text/concatSep.ffg"
      }

let call = fold
      { HttpRequest: \x -> "curl ${x.url}"
      , ShellCommand: \x -> concatSep " " ([ x.executable ] + x.arguments)
      }

in  map call (prompt{ key, text: "Call some tools" })
