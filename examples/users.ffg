# Grace version of an example from https://dhall-lang.org/
let makeUser{ user }=
      let home = "/home/${user}"

      let privateKey = "${home}/.ssh/id_ed25519"

      let publicKey  = "${privateKey}.pub"

      in  { home, privateKey, publicKey }

in  [ makeUser{ user: "bill" }
    , makeUser{ user: "jane" }
    ]
